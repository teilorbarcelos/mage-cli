{
  "name": "mage-cli",
  "version": "1.0.5",
  "description": "CLI to help in web development.",
  "main": "index.js",
  "bin": {
    "mage": "index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/teilorbarcelos/mage-cli.git"
  },
  "author": "Teilor Souza Barcelos <tsb-@live.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "cli-table": "^0.3.6",
    "commander": "^8.2.0",
    "figlet": "^1.5.2",
    "handlebars": "^4.7.8",
    "inquirer": "^8.0.0",
    "shelljs": "^0.8.4"
  },
  "bugs": {
    "url": "https://github.com/teilorbarcelos/mage-cli/issues"
  },
  "homepage": "https://github.com/teilorbarcelos/mage-cli#readme",
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.25",
    "typescript": "^5.4.2"
  },
  "scripts": {
    "build": "rm -rf dist && tsc && yarn copy-package && yarn --cwd dist --production && sudo npm install -g dist && chmod +x /usr/local/bin/mage",
    "copy-package": "cp package.json dist && cp -r src/models dist/models",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "publish": "npm publish",
    "dev": "ts-node src/index.ts"
  },
  "keywords": [
    "cli",
    "mage",
    "automate",
    "development"
  ],
  "files": [
    "dist"
  ]
}